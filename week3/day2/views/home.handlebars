<title>home</title>
<ul>
    <li><a href="/" aria-label="Open home page">Home</a></li>
    <li><a href="./form" style="float: right;" aria-label="add"><img src="data:image/png;base64," style="width: 15%; height: 15%;" alt="add restaurant" class="otherimg"></a></li>
</ul>
<div style="background-image: url(https://www.stone-ideas.com/wordpress/wp-content/uploads/2017/02/Lithea17-1-1.jpg); background-size: 100%;">
<article class="title" aria-label="restaurants"><h6 WAI-ARIA role="heading" aria-level="1">Restaurants</h6></article>
<section style="padding-left: 4%; padding-right: 0;">
    {{#each restaurants}}
        <article style="width: 90%;" aria-label="restaurant {{this.name}}">
            <h6>{{this.name}}</h6>
            {{#each this.menus}}
            <p> - {{this.title}}</p>
            {{/each}}
            <br>
            <a href="./about/{{this.id}}" aria-label="Open about page for {{this.name}}"><img src={{this.image}} alt="image of restaurant"/></a>
            <br>
        </article>
    {{/each}}
</section>
</div>